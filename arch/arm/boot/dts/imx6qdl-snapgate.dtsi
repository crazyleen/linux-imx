/ {
	model = "Wandboard i.MX6 Dual Lite Board";
	compatible = "wand,imx6dl-wandboard", "fsl,imx6dl";

	memory {
		reg = <0x10000000 0x40000000>;
	};
	
	leds {
		compatible = "gpio-leds";

		debug-led {
			label = "Heartbeat";
			gpios = <&gpio7 13 0>;
			linux,default-trigger = "heartbeat";
		};

		debug2-led {
			label = "MMC";
			gpios = <&gpio1 7 1>;
			linux,default-trigger = "mmc";
		};
	};

};

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog_1>;

	hog {
		pinctrl_hog_1: hoggrp-1 {
			fsl,pins = <
				MX6QDL_PAD_EIM_D16__GPIO3_IO16 0x80000000 /* SD3 CD*/
				MX6QDL_PAD_GPIO_18__GPIO7_IO13 0X80000000 /* SYS LED D15 */
				MX6QDL_PAD_GPIO_7__GPIO1_IO07 0X80000000 /* SYS LED D17 */
				MX6QDL_PAD_KEY_COL2__GPIO4_IO10 0X80000000 /* wdt */
			>;
		};
	};

	uart1 {
		pinctrl_uart1_2: uart1grp-1 {
			fsl,pins = <
				MX6QDL_PAD_SD3_DAT7__UART1_TX_DATA 0x1b0b1
				MX6QDL_PAD_SD3_DAT6__UART1_RX_DATA 0x1b0b1
			>;
		};
	};
};

&fec {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_enet_1>;
	phy-mode = "rgmii";
	status = "okay";
};

&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1_2>;
	status = "okay";
};

&usbh1 {
	status = "okay";
};

&usdhc3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc3_2>;
	status = "okay";
};



&usdhc4 {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_usdhc4_1>;
        bus-width = <8>;
        non-removable;
        status = "okay";
};

