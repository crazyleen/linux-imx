/ {
	memory {
		reg = <0x10000000 0x40000000>;
	};
	
	leds {
		compatible = "gpio-leds";

		debug-led {
			label = "sys0";
			gpios = <&gpio7 13 1>;
			linux,default-trigger = "mmc0";
		};

		debug2-led {
			label = "sys1";
			gpios = <&gpio7 8 1>;
			linux,default-trigger = "heartbeat";
		};

		wdt-led {
			label = "wdt";
			gpios = <&gpio4 10 1>;
			linux,default-trigger = "heartbeat";
		};
	};

	gpio-keys {
		compatible = "gpio-keys";
		def_reset {
			label = "Default Reset Button";
			gpios = <&gpio6 16 1>;
			linux,code = <116>; /* KEY_POWER */
			gpio-key,wakeup;
		};
	};

};

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog_1>;

	hog {
		pinctrl_hog_1: hoggrp-1 {
			fsl,pins = <
				MX6QDL_PAD_EIM_D16__GPIO3_IO16 		0X80000000 	/* SD3 CD */
				MX6QDL_PAD_GPIO_18__GPIO7_IO13 		0X80000000  	/* SYS LED D15 */
				MX6QDL_PAD_SD3_RST__GPIO7_IO08 		0X80000000 	/* SYS LED D17 */
				MX6QDL_PAD_KEY_COL2__GPIO4_IO10 	0X80000000 	/* wdt */
				MX6QDL_PAD_NANDF_CS2__GPIO6_IO15 	0X80000000 	/* lan8720 reset */
				MX6QDL_PAD_NANDF_CS1__GPIO6_IO14	0X80000000	/* lan8720 int */
				MX6QDL_PAD_KEY_ROW1__GPIO4_IO09	 	0X80000000 	/* m25p128 spi cs */
				MX6QDL_PAD_NANDF_CS3__GPIO6_IO16	0X80000000 	/* default reset gpio key */
			>;
		};
	};

	enet {
		pinctrl_enet_4: enetgrp-4 {
			fsl,pins = <
				MX6QDL_PAD_ENET_MDIO__ENET_MDIO       	0x1b0b0
				MX6QDL_PAD_ENET_MDC__ENET_MDC         	0x1b0b0
				MX6QDL_PAD_ENET_RXD0__ENET_RX_DATA0	0x1b0b0
				MX6QDL_PAD_ENET_RXD1__ENET_RX_DATA1	0x1b0b0
				MX6QDL_PAD_ENET_CRS_DV__ENET_RX_EN	0x1b0b0
				MX6QDL_PAD_ENET_TXD0__ENET_TX_DATA0	0x1b0b0
				MX6QDL_PAD_ENET_TXD1__ENET_TX_DATA1	0x1b0b0
				MX6QDL_PAD_ENET_TX_EN__ENET_TX_EN	0x1b0b0
				MX6QDL_PAD_ENET_RX_ER__ENET_RX_ER	0x1b0b0
				MX6QDL_PAD_GPIO_16__ENET_REF_CLK      	0x4001b0a8
			>;
		};

	};

	uart1 {
		pinctrl_uart1_2: uart1grp-1 {
			fsl,pins = <
				MX6QDL_PAD_SD3_DAT7__UART1_TX_DATA 0x1b0b1
				MX6QDL_PAD_SD3_DAT6__UART1_RX_DATA 0x1b0b1
			>;
		};
	};
};

&fec {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_enet_4>;
	phy-mode = "rmii";
	phy-reset-gpios = <&gpio6 15 0>;
	phy-reset-duration = <1>;
	#phy-int-gpios = <&gpio6 14 0>;
	status = "okay";
};

&ecspi1 {
	fsl,spi-num-chipselects = <1>;
	cs-gpios = <&gpio4 9 0>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi1_2>;
	status = "okay";

	flash: m25p128@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "st,m25p128";
		spi-max-frequency = <20000000>;
		reg = <0>;
	};
};

&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1_2>;
	status = "okay";
};

&usbh1 {
	status = "okay";
};

&usbotg {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbotg_1>;
	status = "okay";
};

&usdhc4 {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_usdhc4_1>;
        bus-width = <8>;
        non-removable;
        status = "okay";
};

&usdhc3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc3_2>;
	cd-gpios = <&gpio3 16 0>;
	status = "okay";
};


